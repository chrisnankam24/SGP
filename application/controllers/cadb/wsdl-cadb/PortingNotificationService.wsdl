			<fault name="ldbAdministrationServiceFault">
				<soap:fault name="ldbAdministrationServiceFault" use="literal"/>
			</fault>
		</operation>

		<operation name="notifyRejected">
			<soap:operation soapAction="notifyRejected"/>
			<input>
				<soap:body use="literal"/>
			</input>
			<output>
